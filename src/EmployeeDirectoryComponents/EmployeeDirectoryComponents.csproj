<Project Sdk="Microsoft.NET.Sdk.Razor">

  <PropertyGroup>
    <TargetFramework>net6.0</TargetFramework>
    <Nullable>enable</Nullable>
    <ImplicitUsings>enable</ImplicitUsings>
    <Title>EmployeeDirectoryComponents</Title>
    <Authors>Emanuele Bartolesi</Authors>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <PackageProjectUrl>https://github.com/kasuken/.NET-Days-2022-Sample/</PackageProjectUrl>
	<RepositoryUrl>https://github.com/kasuken/.NET-Days-2022-Sample.git</RepositoryUrl>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="EmployeeCard.razor.css" />
  </ItemGroup>

  <ItemGroup>
    <Content Include="EmployeeCard.razor.css" />
  </ItemGroup>


  <ItemGroup>
    <SupportedPlatform Include="browser" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.AspNetCore.Components.Web" Version="6.0.8" />
  </ItemGroup>

</Project>
